{
  "compilerOptions": {
    "target": "es5",
    "module": "amd",
    "lib": ["dom", "dom.iterable", "esnext"],
    "jsx": "preserve",
    "outDir": "./lib",
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "resolveJsonModule": true,
    "declaration": true,
    "diagnostics": true
  }
}
